class Maths_Operations {
    method start() {
        a = -10, b = 25, c = 8

        max_num = max(a, b)
        min_num = min(a, b)
        abs_num = abs(a)
        square_root = sqrt(b)
        cube_root = cbrt(c)
        random_integer = rand_int()
        random_double = rand_double()
        random_char = rand_char()

        // Additional math functions
        power_result = pow(b, 3)  // b^3
        sine_result = sin(30)     // sine of 30 degrees
        logarithm_result = log(100)  // natural logarithm of 100
        round_result = round(5.678, 2)  // rounding to 2 decimal places
        floor_result = floor(8.95)      // floor of 8.95
        ceil_result = ceil(8.01)        // ceil of 8.01

        // Constants
        pi_constant = pi()
        e_constant = e()

        // more math functions can be implemented in later series
    }
}
