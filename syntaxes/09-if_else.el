class If_Else {
    method start() {
       // here if conditions is like in the other langs
       a = 10, b = 15

       // only if
       if(a < b){
            print(a)
       }

       // if-else
       if(a > b) {
            print(a)
       } else {
            print(b)
       }

       // if else if ladder and if else nested are same as other langs

       // short hand if
       print((a == b)? "equal" : "not equal")
    }
}
