# Program that takes an integer input from the user
class UserInput {
    method start() {
        // Prompt the user for an integer input
        print("Enter name:")
        name = input()
        print(name)

        print("Enter age")
        age = input_int()
        print(age)

        print("Enter weight")
        weight = input_double()
        print(weight)

        print("Enter Gender:")
        gender = input_char()
        print(gender)
        
    }
}
